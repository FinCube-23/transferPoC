# Alchemy Configuration
ALCHEMY_API_KEY=your_alchemy_api_key_here
ALCHEMY_NETWORK=sepolia

# Smart Contract Configuration
HONK_VERIFIER_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# Wallet Configuration
WALLET_PRIVATE_KEY=your_wallet_private_key_here

# Server Configuration
PORT=8000

# MongoDB Configuration
# For Docker MongoDB (with authentication):
MONGODB_URI=mongodb://localhost:27017/b2b-membership
# For local MongoDB (without authentication):
# MONGODB_URI=mongodb://localhost:27017/b2b-membership
# RabbitMQ Configuration
# Note: Connects to existing RabbitMQ instance in fincube23_network
# The UMS (User Management System) publishes events to this broker
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_EXCHANGE=exchange.ums.events
RABBITMQ_EXCHANGE_TYPE=topic

# RabbitMQ Consumer Configuration
RABBITMQ_PREFETCH_COUNT=10
RABBITMQ_HANDLER_TIMEOUT=30000
RABBITMQ_RECONNECT_MAX_ATTEMPTS=10
RABBITMQ_RECONNECT_INITIAL_DELAY=1000
RABBITMQ_RECONNECT_MAX_DELAY=60000
